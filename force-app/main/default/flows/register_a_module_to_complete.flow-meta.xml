        <name>Update_Status</name>
        <label>Update Status</label>
        <locationX>176</locationX>
        <locationY>311</locationY>
        <inputAssignments>
            <field>Status__c</field>
            <value>
                <stringValue>Completed</stringValue>
            </value>
        </inputAssignments>
